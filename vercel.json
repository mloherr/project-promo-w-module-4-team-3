{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/public-react/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/public-css/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.ts"
    },
    {
      "src": "/",
      "dest": "/api/public-react/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/api/public-react/$1"
    }
  ]
}
